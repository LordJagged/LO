(#include "./lib/std.lole")
(#include "./lib/file.lole")

(external-fn strtok ((& i8) (& i8)) (& i8))

(fn main () i32
  (let source (read_file "examples/llvm/parse-file.lole"))
  (puts source)

  (puts "----------------------------")

  (puts "first line:")
  (let first_line (strtok source "\n"))
  (puts first_line)

  (puts "second line:")
  (let second_line (strtok (nullptr (& i8)) "\n"))
  (puts second_line)

  0
)
