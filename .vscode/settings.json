{
  "files.associations": {
    "*.lole": "clojure"
  },
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "deno.enable": true,
  "deno.lint": true,
  "deno.unstable": true,
  "cSpell.words": [
    "LLVMIR"
  ],
  "deno.codeLens.testArgs": [
    "--allow-all",
    "--no-check",
    "--unstable"
  ],
}
