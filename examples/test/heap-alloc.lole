(mod lib/cli)

(fn main [] void (
    (puts "Heap/TOTAL_ALLOCATED = ")
    (print_u32 Heap/TOTAL_ALLOCATED) (puts "\n")

    {&p := (Heap/alloc 1)}
    (puts "&p = (Heap/alloc 1) // ")
    (print_u32 &p) (puts "\n")

    (puts "(Heap/free &p)\n")
    (Heap/free &p)

    {&p = (Heap/alloc 1)}
    (puts "&p = (Heap/alloc 1) // ")
    (print_u32 &p) (puts "\n")

    {&p = (Heap/alloc 1)}
    (puts "&p = (Heap/alloc 1) // ")
    (print_u32 &p) (puts "\n")
))
