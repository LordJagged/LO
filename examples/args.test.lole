(mod lib/cli)
(mod lib/args)

(fn main [] [] (
    {args : Args}
    {args = (Args/load)}

    {i : u32}
    {i = 0}
    (loop (
        (if {i == {args . argc}} (break))

        {arg : StringSlice}
        {arg = (Args/get args i)}

        (print_str_slice arg)
        (println)

        {i = {i + 1}}
    ))
))
