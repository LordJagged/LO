(mod lib/std)
(mod lib/wasi)
(mod lib/print)
(mod lib/args)

(mem m1 :min 1000)
(export mem m1 :as memory)

(export main :as _start)
(fn main [] [] (
    {args : Args}
    {args = (Args/load)}

    {i : u32}
    {i = 0}
    (loop (
        (if {i == {args . argc}} (break))

        {arg : StringSlice}
        {arg = (Args/get args i)}

        (print_str_slice arg)
        (println)

        {i = {i + 1}}
    ))
))
